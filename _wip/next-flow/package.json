    "@web3auth/metamask-adapter": "^4.6.0",
    "@web3auth/openlogin-adapter": "^4.6.0",
    "@web3auth/phantom-adapter": "^4.6.0",
    "@web3auth/solana-provider": "^4.6.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "framer-motion": "^10.2.4",
    "lodash": "^4.17.21",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
