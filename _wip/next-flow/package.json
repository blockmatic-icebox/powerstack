    "@heroicons/react": "^2.0.16",
    "@hookform/resolvers": "^2.9.11",
    "@onflow/fcl": "^1.3.2",
    "@onflow/types": "^1.0.5",
    "@solana/web3.js": "^1.73.3",
    "@tailwindcss/forms": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
