{
  "name": "powerstack-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ts": "concurrently \"yarn ts:watch\" \"sleep 1 && yarn dev\"",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "compose": "env-cmd -f .env.compose next dev",
    "https": "concurrently \"yarn ssl\" \"next dev -p 3001\"",
    "https:ts": "concurrently \"yarn ssl\" \"next dev -p 3001\" \"yarn ts:watch\" ",
    "ssl": "local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql": "env-cmd -f ./app-engine/graphql/.env.codegen graphql-codegen --config ./app-engine/graphql/codegen.yml && eslint --fix ./app-engine/graphql/generated-sdk.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "theme": "env-cmd -f ./.env.development ts-node ./_scripts/theme-gen/index.ts",
    "lhci:mobile": "lhci autorun",
    "lhci:desktop": "lhci autorun --collect.settings.preset=desktop"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@blockmatic/prettier-config": "^2.1.0",
    "@cloudinary/url-gen": "^1.8.6",
    "@hookform/resolvers": "^2.9.7",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@sentry/browser": "^7.11.1",
    "@solana/web3.js": "^1.53.0",
    "@stitches/react": "^1.2.8",
    "@types/circular-json": "^0.4.0",
    "@web3auth/base": "^1.2.0",
    "@web3auth/web3auth": "^1.2.0",
    "anchor-link": "^3.5.1",
    "anchor-link-browser-transport": "^3.5.1",
    "apollo-link-sentry": "^3.2.0",
    "auto-zustand-selectors-hook": "^2.0.0",
    "bs58": "^5.0.0",
    "camelcase-keys": "^8.0.1",
    "circular-json": "^0.5.9",
    "cloudinary-build-url": "^0.2.4",
    "coingecko-api-v3": "^0.0.13",
    "cookies-next": "^2.1.1",
    "decimal.js": "^10.4.0",
    "env-var": "^7.1.1",
    "ethers": "^5.7.0",
    "framer-motion": "^7.2.0",
    "global": "^4.4.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.10.1",
    "iron-session": "^6.1.3",
    "jwt-decode": "^3.1.2",
    "lighthouse": "^9.6.6",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-datocms": "^3.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-json-view": "^1.21.3",
    "react-use": "^17.4.0",
    "safe-json-stringify": "^1.2.0",
    "simple-zustand-devtools": "^1.1.0",
    "zod": "^3.18.0",
    "zustand": "^4.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-generic-sdk": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@lhci/cli": "^0.9.0",
    "@types/cli-spinner": "^0.2.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "cli-spinner": "^0.2.10",
    "concurrently": "^7.3.0",
    "cross-fetch": "^3.1.5",
    "env-cmd": "^10.1.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "local-ssl-proxy": "^1.3.0",
    "ts-node": "^10.4.0",
    "typescript": "4.7.4"
  }
}
