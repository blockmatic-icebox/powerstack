{
  "private": true,
  "name": "powerstack",
  "scripts": {
    "build": "turbo build --no-daemon",
    "dev": "turbo dev --no-daemon --parallel",
    "lint": "turbo lint --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.5"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*",
    "contracts/*"
  ],
  "dependencies": {
    "eosio-core": "^0.0.2",
    "next-auth": "^4.22.1"
  }
}
