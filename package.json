{
  "name": "powerstack-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ts": "concurrently \"yarn ts:watch\" \"sleep 1 && yarn dev\"",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "compose": "env-cmd -f .env.compose next dev",
    "https": "concurrently \"yarn ssl\" \"next dev -p 3001\"",
    "https:ts": "concurrently \"yarn ssl\" \"next dev -p 3001\" \"yarn ts:watch\" ",
    "ssl": "local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\""
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@blockmatic/prettier-config": "^2.1.0",
    "@cloudinary/url-gen": "^1.8.6",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "^2.9.10",
    "@solana/web3.js": "^1.70.1",
    "@tailwindcss/forms": "^0.5.3",
    "@web3auth/base": "^3.1.0",
    "@web3auth/core": "^4.0.0",
    "@web3auth/metamask-adapter": "^4.0.0",
    "@web3auth/openlogin-adapter": "^4.0.0",
    "@web3auth/phantom-adapter": "^4.0.0",
    "@web3auth/solana-provider": "^4.0.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "framer-motion": "^7.6.12",
    "iron-session": "^6.3.1",
    "lodash": "^4.17.21",
    "next": "12.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.5",
    "react-use": "^17.4.0",
    "tailwindcss": "^3.2.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "local-ssl-proxy": "^1.3.0",
    "postcss": "^8.4.19",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  }
}
